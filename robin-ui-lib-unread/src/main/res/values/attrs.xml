<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BaseUnreadView">
        <attr name="unread_alwaysShowZero" format="boolean" />
        <attr name="unread_anim_type">
            <enum name="none" value="-1" />
            <enum name="scale" value="1" />
            <enum name="shake" value="2" />
            <enum name="transit" value="3" />
        </attr>
        <attr name="unread_icon" format="reference" />
        <attr name="unread_count_textColor" format="color" />
        <attr name="unread_count_textSize" format="dimension|reference" />
        <attr name="unread_count_textBackground" format="reference" />
        <attr name="unread_layout" format="reference" />
        <attr name="unread_label" format="reference|string" />
    </declare-styleable>
</resources>